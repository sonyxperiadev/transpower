android_app {
    name: "TransPowerSensors",
    certificate: "platform",
    privileged: true,

    sdk_version: "system_current",
    proprietary: true,

    srcs: ["src/**/*.java"],

    static_libs: [
        "transpower_libacc",
        "transpower_libpower",
        "transpower_libprox",
        "TransPowerCommon_res",
    ],

    aaptflags: ["--auto-add-overlay"],

    optimize: {
        proguard_flags_files: [
            "proguard.flags",
        ],
    },
}
